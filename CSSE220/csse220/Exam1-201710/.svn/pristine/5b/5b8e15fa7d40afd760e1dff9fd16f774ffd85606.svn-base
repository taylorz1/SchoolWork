
public class TestThisClass {

	/**
	 * Converts a double to an int with rounding.
	 * If the double is < .5, rounds down
	 * If the double is >= .5, rounds up
	 *
	 * @param input
	 * @return double converted to int
	 */
	public static int roundDoubleToInt(double input) {
		int rawInt = (int) input;
		if(input - rawInt < 0.5) {
			return rawInt;
		} 
		return rawInt + 1;
	}
}
